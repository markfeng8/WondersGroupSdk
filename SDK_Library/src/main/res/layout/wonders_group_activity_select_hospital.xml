<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/wonders_rgb_color_ffffff"
    android:orientation="vertical"
    tools:context=".ui.selecthospital.view.SelectHospitalActivity">

    <com.wondersgroup.android.jkcs_sdk.widget.TitleBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text="@string/wonders_select_hospital_please"
        app:textColor="@color/wonders_rgb_color_ffffff"
        app:textSize="@dimen/wonders_text_size_16sp"
        app:titleBgColor="@color/wonders_rgb_color_1e90ff" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/wonders_rgb_color_c9c9c9"
        android:dividerHeight="1dp"
        android:scrollbars="none" />

</LinearLayout>