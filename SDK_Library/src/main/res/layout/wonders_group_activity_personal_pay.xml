<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/wonders_rgb_color_ffffff"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context=".ui.personalpay.PersonalPayActivity">

    <com.wondersgroup.android.jkcs_sdk.widget.TitleBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text="@string/wonders_personal_pay"
        app:textColor="@color/wonders_rgb_color_ffffff"
        app:textSize="@dimen/wonders_text_size_16sp"
        app:titleBgColor="@color/wonders_rgb_color_1e90ff" />

    <include layout="@layout/wonders_group_personal_payment_result" />

    <include layout="@layout/wonders_group_payment_success" />

    <!--<include layout="@layout/wonders_group_payment_failed" />-->

</LinearLayout>